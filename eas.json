{
  "cli": {
    "version": ">= 0.34.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "node": "18.16.0",
      "channel": "production",
      "autoIncrement": true
    },
    "staging": {
      "extends": "production",
      "distribution": "internal",
      "channel": "staging"
    },
    "development": {
      "extends": "production",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }    
    }
  },
  "submit": {
